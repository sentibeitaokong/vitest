{
  "name": "vitest-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "test": "vitest",
    "test:update": "vitest --update",
    "coverage": "vitest run --coverage",
    "vite:preview": "npx vite preview --outDir ./html",
    "debugger": "ndb npm run dev",
    "browser:debugger": "vitest --inspect-brk --config=vitest.browser.config.ts  --browser --no-file-parallelism",
    "reporter": "vitest --reporter=verbose",
    "playwright": "playwright show-trace \"playwright-traces\"",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:browser": "vitest --config=vitest.browser.config.ts"
  },
  "dependencies": {
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-istanbul": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "axios": "^1.13.2",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.3.0",
    "json-server": "^1.0.0-beta.3",
    "memfs": "^4.51.1",
    "npm-run-all2": "^8.0.4",
    "typescript": "~5.9.0",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.16",
    "vitest-browser-vue": "^2.0.1",
    "vue-router": "^4.6.4",
    "vue-tsc": "^3.1.5",
    "vuex": "^4.1.0"
  }
}
